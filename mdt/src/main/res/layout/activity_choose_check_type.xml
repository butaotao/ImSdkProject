<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dachen.mdt.activity.order.ChooseCheckTypeActivity">

    <RelativeLayout style="@style/Common_Title_style">

        <Button
            android:id="@+id/back_btn"
            style="@style/ActionBar_BackBtn"/>

        <TextView
            android:id="@+id/title"
            style="@style/ActionBar_Title"
            android:text="添加检查结果"/>

        <ImageView
            style="@style/title_divider_line"
            />

        <TextView
            android:id="@+id/right_btn"
            style="@style/ActionBar_RightText"
            android:visibility="gone"
            android:text="确定"
            />
    </RelativeLayout>

    <ListView
        android:footerDividersEnabled="true"
        android:id="@+id/list_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</LinearLayout>
